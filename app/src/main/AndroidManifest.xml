<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.epyco.matchup">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_matchup"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_matchup_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MatchUp">
        <activity android:name=".ListMatchUpsView">
        </activity>
        <activity android:name=".ActivityInfo">
        </activity>
        <activity android:name=".SearchMatchUpsView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-9983989988655125~9054385791"/>
    </application>

</manifest>